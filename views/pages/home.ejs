<!DOCTYPE html>
<html data-theme="<%= theme ?? 'purple' %>" data-font="<%= font ?? 'Lato' %>" lang="en">

<head>
  <%- include('../partials/head.ejs') %>
  <title>Home ‚ù§Ô∏è 2k17</title>
  <link rel="stylesheet" href="/styles/css/home.css" />
  <link rel="stylesheet" href="/styles/css/quill/min.css" />
  <link rel="manifest" href="/manifest.json">
  <meta name="theme-color" content="#1f1c2e">
  <meta name="accent-color" content="<%= theme? theme : 'purple' %>">
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <link rel="icon" sizes="192x192" href="/images/icons/logo_192x192.png">
  <script src="/js/quill/min.js"></script>

</head>

<body>

  <%- include('../partials/navbar') %>
  <%- include('../partials/loader') %>
  <main class="loader-after-body" style="display: none;">
    <!-- Hero Section -->
    <section class="hero">
      <div class="hero-text">
        <!--ganesha theme <h2 id="greeting"><span id="greeting-msg"></span>, <%= user.name.split(' ')[0] %></h2>-->
        <h2 id="greeting">Happy Ganesh Chaturthi!</h2>
        <!--ganesha theme <p>Click the button below to know about latest features and improvements</p><br>-->
        <p>May bappa bless you with happiness and joy. Click on ganesha in corner and catch some modak to eat üòã</p><br>
        <!--ganesha theme <a class="cta-btn" href="/whatsnew"><span class="fal fa-sparkle"></span> &nbsp; &nbsp; What's new</a>-->
        <a class="cta-btn" href="#" onclick="makeConfetti()">‡§ó‡§£‡§™‡§§‡§ø ‡§¨‡§æ‡§™‡•ç‡§™‡§æ ‡§Æ‡•ã‡§∞‡§Ø‡§æ &nbsp;üéâ</a>
      </div>
      <div class="featured-photo">
        <!--ganesha theme <img src="<%= featuredPhoto.url %>" alt="Today's featured photo" class="featured-photo-img">-->
        <img src="/images/ganesha_featured_image.jpg" alt="Ganesha Image" class="featured-photo-img">
        <div class="overlay-text">Featured photo</div>
      </div>
    </section>

    <% if (birthdays.length > 0) { %>
    <section id="birthday-spotlight" class="no-select">
      <h3><span class="fal fa-cake-candles"></span> Birthday Spotlight</h3>

      <div class="gallery-grid">
        <% birthdays.forEach(bdayuser => { %>
        <div class="image-card" style="padding: 1rem;" onclick="window.location.href='/<%= bdayuser.username %>'">
          <img src="<%- bdayuser.profile || '/images/user.png' %>" alt="<%= bdayuser.name %>'s photo">
          <h4 style="margin-bottom: 4px;"><%= bdayuser.name %> <%- bdayuser.isBirthdayToday ? 'üéÇ' : '' %></h4>
          <p><%= bdayuser.username %></p>
          <% if(bdayuser.isBirthdayToday){ %>
          <p style="line-height: 26px;">Happy Birthday! üéâ</p>
          <% } else { %>
          <p style="line-height: 26px;"><%- formatDOB(bdayuser.dob) %></p>
          <% } %>
        </div>
        <% }) %>
      </div>
    </section>
    <% } %>
    <br>
    <div class="pwa-banner" id="pwaBanner" style="display: none;">
      <div class="text">
        <div class="title"><span class="fal fa-mobile-screen-button"></span> 2k17 is now an app</div>
        <div class="subtitle">Get exciting features</div>
      </div>
      <button class="install-btn" id="installBtn">Install</button>
    </div>

    <!-- ganesha theme -->
    <div class="competition">
      <div class="competition-box">
        <h3>2k17 Loves Ganesha ‚ù§Ô∏è</h3>
        <p>Share photos of Ganpati decoration near you with everyone and win exciting badges. Decoration with most likes will get a special badge üòÅ</p>
        <button onclick="openPostPopup()">Share now!</button>
      </div>
    </div>

    <br class="small-only">

    <div class="create-post-box small-only" onclick="openPostPopup()">
      <p class="heading">Create a new post</p>
      <div class="pseudo-post-box">
        <input readonly type="text" placeholder="What's on your mind?">
        <span class="fal fa-image"></span>
      </div>
    </div>
    <span class="small-only"><br></span>

    <section class="posts" id="posts">
    </section>

    <br><br><br><br><br>
  </main>

  <div id="comments-popup">
    <div class="overlay" onclick="hidePopup()" id="overlay"></div>
    <div class="comments-box">
      <div class="heading">
        <p>Comments</p>
        <p class="close-icon fas fa-times" onclick="hidePopup()"></p>
      </div>
      <div class="comments-list" id="comments-list">
      </div>
      <div id="editor" class="editor" contenteditable="true" placeholder="Express your opinion..."></div>
      <ul id="mentionBox" class="mention-box hidden" style="margin: 14rem 0 0 0.75rem;"></ul>
      <div class="input-area comment-container">
        <input id="new-comment" autocomplete="off" placeholder="Comment (Mention using @username)..." />
        <button type="button" id="newCommentBtn">Post</button>
      </div>
    </div>
  </div>

  <div id="post-popup" class="popup-wrapper">
    <div class="overlay" onclick="closePostPopup()"></div>
    <div class="post-box">
      <div class="heading">
        <p>Create a new post</p>
        <p class="close-icon fal fa-xmark" onclick="closePostPopup()"></p>
      </div>
      <span class="grey-1" style="margin: 1rem 0;"><span class="fal fa-info-circle"></span> &nbsp;You can mention people by typing @ followed by their name or username. Use @all to mention everyone.</span>
      <div id="editor2" class="editor" placeholder="Say hi to everyone..."></div>
      <textarea id="hiddenContent" name="content" hidden></textarea>
      <ul id="mentionBox" class="mention-box hidden"></ul>
      <input type="file" id="media-input" accept="image/*">
      <label for="media-input" class="upload-label"><span class="fal fa-upload"></span> &nbsp; Add photo</label>
      <div class="preview" id="media-preview"></div>

      <div class="action-buttons">
        <button class="cancel-btn" onclick="closePostPopup()">Cancel</button>
        <button class="create-post-btn" onclick="createPost('<%= user._id %>')">Post</button>
      </div>
    </div>
  </div>

  <div id="post-likes-popup">
    <div class="overlay" onclick="closePostLikes()"></div>
    <div class="post-likes-box">
      <div class="heading">
        <p class="title"><span class="fal fa-heart"></span> Likes on post</p>
        <p class="close-icon fal fa-xmark" onclick="closePostLikes()"></p>
      </div>
      <div class="post-likes-list" id="post-likes-list">

      </div>
    </div>
  </div>

  <!-- ganesha theme virtual prasad -->
  <div id="virtual-prasad" onclick="window.location.href='/modak'">
