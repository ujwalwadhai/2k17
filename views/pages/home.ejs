  <%- include('../partials/footer.ejs') %>

  <div id="notification-banner" style="display: none;">
    <p>Would you like to recieve important notifications from 2k17 üíå?</p>
    <div class="actions">
      <button id="later-btn">Maybe Later</button>
      <button id="grant-btn" onclick="requestNotificationPermissionAndSubscribe()">Yes</button>
    </div>
  </div>

  <script>
    document.addEventListener('DOMContentLoaded', () => {
      loadPosts();
      document.getElementById('media-input').addEventListener('change', function() {
        var preview = document.getElementById('media-preview');
        preview.innerHTML = '';
        Array.from(this.files).forEach(file => {
          var url = URL.createObjectURL(file);
          var el = file.type.startsWith('image') ?
            `<img src="${url}" />` :
            `<video src="${url}" muted autoplay loop />`;
          preview.innerHTML += el;
        });
      });
    })
  </script>
  <script src="/js/all.js"></script>
  <script src="/js/notifications.js"></script>
  <script src="/js/comment.js"></script>
  <script src="/js/createPost.js"></script>
  <script src="/js/components/confetti.js"></script>

  <script>
    let deferredPrompt;

    window.addEventListener('beforeinstallprompt', (e) => {
      e.preventDefault();
      deferredPrompt = e;

      const banner = document.getElementById('pwaBanner');
      banner.style.display = 'flex';

      document.getElementById('installBtn').addEventListener('click', async () => {
        if (deferredPrompt) {
          deferredPrompt.prompt();
          const {
            outcome
          } = await deferredPrompt.userChoice;
          deferredPrompt = null;
        }
      });
    });

    window.addEventListener('appinstalled', () => {
      document.getElementById('pwaBanner').style.display = 'none';
    });

    function getGreeting() {
      var now = new Date();
      var hours = now.getHours();

      if (hours >= 5 && hours < 12) {
        return "Good morning";
      } else if (hours >= 12 && hours < 17) {
        return "Good afternoon";
      } else if (hours >= 17 && hours < 21) {
        return "Good evening";
      } else {
        return "Welcome"; // No greeting (e.g., for night time)
      }
    }

    var greeting = getGreeting();
    var greetingElement = document.getElementById("greeting-msg")
    if (greetingElement) greetingElement.innerHTML = greeting;

    // ganesha theme confetti function
    function makeConfetti() {
      Confetti({
        origin: {
          x: 0.15,
          y: 0
        },
        colors: ['#FF9933', '#F47820', '#FFC107', '#FFD700', '#E32636'],
        duration: 5000,
        spawnRate: 8
      });

      Confetti({
        origin: {
          x: 0.85,
          y: 0
        },
        colors: ['#FF9933', '#F47820', '#FFC107', '#FFD700', '#E32636'],
        duration: 5000,
        spawnRate: 8
      });
    }



    let quill;

    document.addEventListener('DOMContentLoaded', () => {

      // ganesha theme confetti
      makeConfetti()


      // Prevent re-initializing
      if (quill) return;

      quill = new Quill('#editor2', {
        theme: 'snow', // Use the 'snow' theme for a clean toolbar
        placeholder: 'Say hi to everyone...',
        modules: {
          // Configure the toolbar for bold, italics, underline, and links
          toolbar: [
            ['bold', 'italic', 'underline'],
            ['link']
          ]
        }
      });
    })
  </script>

